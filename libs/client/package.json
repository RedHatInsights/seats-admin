{
  "name": "client",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/client.js",
  "types": "./dist/client.d.ts",
  "exports": {
    ".": {
      "import": "./dist/client.js",
      "require": "./dist/client.umd.cjs",
      "types": "./dist/client.d.ts"
    }
  },
  "scripts": {
    "postinstall": "node kiota-gen.js",
    "build": "wireit"
  },
  "wireit": {
    "build": {
      "command": "vite build",
      "files": [
        "api/**",
        "service.ts",
        "package.json",
        "tsconfig.json",
        "vite.config.ts"
      ],
      "output": [
        "dist/**"
      ]
    }
  },
  "dependencies": {
    "@microsoft/kiota-abstractions": "1.0.0-preview.14",
    "@microsoft/kiota-http-fetchlibrary": "^1.0.0-preview.15",
    "@microsoft/kiota-serialization-form": "^1.0.0-preview.6",
    "@microsoft/kiota-serialization-json": "^1.0.0-preview.15",
    "@microsoft/kiota-serialization-text": "^1.0.0-preview.14"
  },
  "devDependencies": {
    "decompress": "^4.2.1",
    "follow-redirects": "^1.15.2"
  }
}
