{
  "name": "client",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/client.js",
  "types": "./dist/client.d.ts",
  "exports": {
    ".": {
      "import": "./dist/client.js",
      "require": "./dist/client.umd.cjs",
      "types": "./dist/client.d.ts"
    }
  },
  "scripts": {
    "build": "wireit"
  },
  "wireit": {
    "build": {
      "command": "vite build",
      "files": [
        "api/**",
        "entitlements-api/**",
        "service.ts",
        "package.json",
        "tsconfig.json",
        "vite.config.ts"
      ],
      "output": [
        "dist/**"
      ]
    }
  },
  "devDependencies": {
    "oazapfts": "^4.6.0"
  }
}
